@model User
<h1>Register</h1>
<p>Already registered? <a href="/">Login here</a></p>
<form action="create" method="POST">
<div class="form-group">
    <span asp-validation-for="Fname"></span>
    <label asp-for="Fname"></label>
    <input asp-for="Fname">
</div>

<div class="form-group">
    <span asp-validation-for="Lname"></span>
    <label asp-for="Lname"></label>
    <input asp-for="Lname">
</div>

<div class="form-group">
    <span asp-validation-for="Email"></span>
    <label asp-for="Email">Email</label>
    <input asp-for="Email">
</div>
<div class="form-group">
    <span asp-validation-for="DOB"></span>
    <label asp-for="DOB"></label>
    <input asp-for="DOB">
</div>
<div class="form-group">
    <span asp-validation-for="Password"></span>
    <label asp-for="Password">Password</label>
    <input asp-for="Password">
</div>
<div class="form-group">
    <span asp-validation-for="Confirm"></span>
    <label asp-for="Confirm">Confirm</label>
    <input asp-for="Confirm">
</div>
    <input type="submit" value="Register">
</form>

